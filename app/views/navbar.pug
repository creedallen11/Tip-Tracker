- var navOptions = ['Global Data', 'Account', 'Logout'];

nav
  div.nav-wrapper.blue-grey
    a.brand-logo(href="/") Tip Tracker
    a.button-collapse(href="#", data-activates="slide-out")
      i.material-icons menu
    if isAuth  
      ul.right.hide-on-med-and-down
        for item in navOptions
          if item === 'Account'
            li
              a(href='/settings')= item
          else if item === 'Global Data'
            li
              a(href='/data')= item
          else
            li
              a(href='/logout')= item
      ul.side-nav#slide-out
        for item in navOptions
          if item === 'Account'
            li
              a(href='/settings')= item
          else if item === 'Global Data'
            li
              a(href='/data')= item
          else
            li
              a(href='/logout')= item
    else
      ul.right.hide-on-med-and-down
        li
          a(href='/login') Login
      ul.side-nav#slide-out
        li
          a(href='/login') Login